<div class="structure-{type}">
    {?$key}
        <h5>{$key}</h5>
    {/$key}
    {?fields}
    <ul>
        {@hashmap map=fields}
        <li>
            {>"cms/cms/structure"/}
        </li>
        {/hashmap}
    </ul>
    {/fields}
    {?items}
    <ul>
        {@list list=items}
        <li>
            {>"cms/cms/structure"/}
        </li>
        {/list}
    </ul>
    {/items}
</div>
